    "keywords": ["PDF", "PHPPdf"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "Frank Beentjes",
            "homepage": "http://php-programmer.nl",
            "email":    "frankbeen@gmail.com"
        }     
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "~3.0|~3.3",
        "psliwa/php-pdf": "^1.1.5",
        "sensio/framework-extra-bundle": ">=2.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4,<6.0.0"
